{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "username",
    "password",
    "email"
  ],
  "properties": {
    "username": {
      "$id": "#/properties/username",
      "type": "string",
      "title": "The Username Schema",
      "default": "",
      "examples": [
        "Alex"
      ],
      "pattern": "^(.*)$"
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The Password Schema",
      "default": "",
      "examples": [
        "dksaASD"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "bruh@suh.com"
      ],
      "pattern": "^(.*)$"
    },
    "photo_url": {
      "$id": "#/properties/photo_url",
      "type": "string",
      "title": "The Photo_url Schema",
      "default": "",
      "examples": [
        "https://myphoto.com"
      ],
      "pattern": "^(.*)$"
    }
  }
}